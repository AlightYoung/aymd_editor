{
  "name": "aymd_editor",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron-main.js",
  "description": "a markdown editor using react & electron",
  "dependencies": {
    "highlight.js": "^11.7.0",
    "marked": "^4.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently -k \"npm start\" \"npm run electron\"",
    "electron": "wait-on tcp:3000 && electron ."
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "electron": "^22.0.0",
    "electron-is-dev": "^2.0.0",
    "wait-on": "^6.0.1"
  }
}
